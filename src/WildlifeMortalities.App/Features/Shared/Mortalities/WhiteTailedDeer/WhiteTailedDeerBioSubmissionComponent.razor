@using static BioSubmissionDialog;
@using WildlifeMortalities.Data.Entities.BiologicalSubmissions

<MudCheckBox @bind-Checked="BioSubmission.IsHornIncluded" Label="Horn included?" For="() => BioSubmission.IsHornIncluded" Disabled="IsReadOnly" />
<MudCheckBox @bind-Checked="BioSubmission.IsHeadIncluded" Label="Head included?" For="() => BioSubmission.IsHeadIncluded" Disabled="IsReadOnly" />

@code {

    [Parameter]
    public bool IsReadOnly { get; set; } = false;

    [Parameter]
    [EditorRequired]
    public WhiteTailedDeerBioSubmission BioSubmission { get; set; } = null!;

    public class WhiteTailedDeerBioSubmissionValidator : BioSubmissionValidator<WhiteTailedDeerBioSubmission>
    {
    }

}
