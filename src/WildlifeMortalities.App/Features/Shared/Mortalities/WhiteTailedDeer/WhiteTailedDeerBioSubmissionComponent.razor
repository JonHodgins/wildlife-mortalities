@using WildlifeMortalities.Data.Entities.BiologicalSubmissions
@using static WildlifeMortalities.App.Features.Shared.Mortalities.BioSubmissionAnalysisDialog;

<MudCheckBox @bind-Checked="BioSubmission.IsHornProvided" Label="Horn included?" For="() => BioSubmission.IsHornProvided" Disabled="IsReadOnly" />
<MudCheckBox @bind-Checked="BioSubmission.IsHeadProvided" Label="Head included?" For="() => BioSubmission.IsHeadProvided" Disabled="IsReadOnly" />

@code {

    [Parameter]
    public bool IsReadOnly { get; set; } = false;

    [Parameter]
    [EditorRequired]
    public WhiteTailedDeerBioSubmission BioSubmission { get; set; } = null!;

    public class WhiteTailedDeerBioSubmissionValidator : BioSubmissionValidator<WhiteTailedDeerBioSubmission>
    {
    }

}
