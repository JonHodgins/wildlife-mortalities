<MudCheckBox @bind-Checked="BioSubmission.IsHornIncluded" Label="Horn included?" For="() => BioSubmission.IsHornIncluded"/>
<MudCheckBox @bind-Checked="BioSubmission.IsHeadIncluded" Label="Head included?" For="() => BioSubmission.IsHeadIncluded"/>

@code {

    [Parameter]
